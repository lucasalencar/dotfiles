[user]
  name = AUTHOR NAME
  email = AUTHOR EMAIL

[core]
  editor = mate -w
  excludesfile = ~/.gitignore

[color]
  ui = true
  diff = auto
  status = auto
  branch = auto

[alias]
  st = status
  co = checkout
  com = commit
  comm = commit -m
  br = branch

[credential]
  helper = osxkeychain

[push]
	default = simple
